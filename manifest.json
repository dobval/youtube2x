{
  "manifest_version": 2,
  "name": "YouTube2x",
  "version": "1.0",
  "description": "Doubles the playback on YouTube videos",
  "icons": {
          "48": "/android-chrome-192x192.png"
       },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*", "*://*.youtu.be/*"],
      "js": ["youtube2x.js"]
    }
  ]
}
